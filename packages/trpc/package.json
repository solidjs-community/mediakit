{
  "name": "@solid-mediakit/trpc",
  "description": "tRPC integration for Solid 2",
  "version": "2.0.1",
  "scripts": {
    "build": "tsc -p . && rollup -c && node scripts/postbuild.mjs",
    "clean": "pnpm rm -rf ./node_modules .turbo .solid handler.*",
    "lint": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index/index.jsx",
  "module": "./dist/index/index.jsx",
  "exports": {
    ".": {
      "types": "./dist/index/index.d.ts",
      "import": "./dist/index/index.jsx",
      "default": "./dist/index/index.jsx"
    },
    "./handler": {
      "types": "./handler.d.ts",
      "import": "./handler.js",
      "default": "./handler.js"
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@solidjs/router": "^0.11.2",
    "@solidjs/start": "^0.5.2",
    "solid-js": "^1.8.14",
    "vinxi": "^0.2.1",
    "@tanstack/solid-query": "^5.0.0",
    "@trpc/client": "^10.41.0",
    "@trpc/server": "^10.41.0",
    "@types/babel__core": "7.20.0",
    "@types/node": "^18.7.14",
    "@typescript-eslint/parser": "^5.44.0",
    "rollup": "^4.9.6",
    "rollup-preset-solid": "^2.0.1",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "@solidjs/router": "^0.11.2",
    "@solidjs/start": "^0.5.2",
    "solid-js": "^1.8.14",
    "vinxi": "^0.2.1",
    "@tanstack/solid-query": "^5.0.0",
    "@trpc/client": "^10.41.0",
    "@trpc/server": "^10.41.0"
  },
  "engines": {
    "node": ">=16"
  },
  "author": "OrJDev"
}
