{
  "name": "@solid-mediakit/prpc",
  "description": "A typesafed Wrapper for Solid's RPC protocol",
  "version": "1.3.0",
  "scripts": {
    "build": "rimraf dist && tsup --config ./tsup.config.ts",
    "clean": "pnpm clean:dist && rimraf ./node_modules .turbo .solid",
    "lint": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "clean:dist": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "files": [
    "src/**/*",
    "dist/**/*",
    "provider.d.ts",
    "provider.jsx",
    "provider.js",
    "provider.cjs"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./provider": {
      "import": "./provider.js",
      "require": "./provider.cjs",
      "types": "./provider.d.ts"
    }
  },
  "main": "./dist/server.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/server.js",
  "devDependencies": {
    "@tanstack/solid-query": "^5.51.15",
    "@types/node": "^22.0.0",
    "@typescript-eslint/parser": "^7.18.0",
    "tsup": "^8.2.3",
    "tsup-preset-solid": "2.2.0",
    "typescript": "^5.5.4",
    "zod": "^3.23.8",
    "@solidjs/router": "^0.14.5",
    "@solidjs/start": "^1.0.6",
    "solid-js": "^1.8.19",
    "vinxi": "^0.4.3",
    "@solidjs/meta": "^0.29.4"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/pluginutils": "^5.1.0"
  },
  "peerDependencies": {
    "@tanstack/solid-query": "^5.56.2",
    "typescript": "^4.8.2",
    "zod": "^3.22.4",
    "@solidjs/router": "^0.14.5",
    "@solidjs/start": "^1.0.6",
    "solid-js": "^1.8.19",
    "vinxi": "^0.4.3",
    "@solidjs/meta": "^0.29.4"
  },
  "engines": {
    "node": ">=16"
  },
  "browser": {
    "./dist/server.js": "./dist/index.js",
    "./dist/server.cjs": "./dist/index.cjs"
  },
  "typesVersions": {}
}
