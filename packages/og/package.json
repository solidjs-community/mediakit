{
  "name": "@solid-mediakit/og",
  "version": "0.3.2",
  "type": "module",
  "description": "An Open Graph / Dynamic image utility library for Solid.",
  "main": "./dist/index.jsx",
  "module": "./dist/index.jsx",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.jsx"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server.js"
    },
    "./unplugin": {
      "import": "./dist/unplugin.js",
      "types": "./dist/unplugin/index.d.ts",
      "default": "./dist/unplugin.js"
    }
  },
  "scripts": {
    "build": "tsc && tsup",
    "test": "vitest run --globals",
    "clean": "rimraf dist node_modules"
  },
  "keywords": [
    "MediaKit",
    "SolidJS",
    "Og"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/types": "7.27.3",
    "@solidjs/start": "^1.1.3",
    "@types/babel-types": "^7.0.16",
    "@types/babel__core": "7.20.5",
    "babel-plugin-tester": "^11.0.4",
    "jiti": "^2.4.2",
    "tsup": "^8.2.3",
    "typescript": "^5.8.3",
    "vitest": "^3.0.4"
  },
  "dependencies": {
    "@babel/core": "7.27.3",
    "@solid-mediakit/shared": "workspace:*",
    "@vercel/og": "^0.6.8",
    "satori-html": "^0.3.2",
    "unplugin": "^2.3.5"
  },
  "peerDependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/start": "^1.1.3",
    "solid-js": "^1.9.5"
  }
}
